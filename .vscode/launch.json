  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!**/node_modules/**"
      ],
      "program": "${workspaceFolder}/src/index.ts"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!**/node_modules/**"
      ],
      "program": "${workspaceFolder}/src/shcema.decorator.ts"
    }
  ]
}